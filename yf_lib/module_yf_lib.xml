  <target name="compile.module.yf_lib" depends="compile.module.yf_lib.production,compile.module.yf_lib.tests" description="Compile module yf_lib"/>
  
  <target name="compile.module.yf_lib.production" depends="register.custom.compilers" description="Compile module yf_lib; production classes">
    <mkdir dir="${yf_lib.output.dir}"/>
    <javac2 destdir="${yf_lib.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.yf_lib}/javac">
      <compilerarg line="${compiler.args.yf_lib}"/>
      <bootclasspath refid="yf_lib.module.bootclasspath"/>
      <classpath refid="yf_lib.module.production.classpath"/>
      <src refid="yf_lib.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.yf_lib"/>
    </javac2>
    
    <copy todir="${yf_lib.output.dir}">
      <fileset dir="${module.yf_lib.basedir}/build/generated/source/r/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.yf_lib.basedir}/build/generated/source/aidl/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.yf_lib.basedir}/build/generated/source/buildConfig/debug">
