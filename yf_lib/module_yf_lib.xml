        <type type="file"/>
      </fileset>
      <fileset dir="${module.yf_lib.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.yf_lib.tests" depends="register.custom.compilers,compile.module.yf_lib.production" description="compile module yf_lib; test classes" unless="skip.tests">
    <mkdir dir="${yf_lib.testoutput.dir}"/>
